#Open Babel Makefile

prefix        = @prefix@
exec_prefix   = @exec_prefix@
pkgdatadir    = @datadir@/openbabel
bindir        = @bindir@

DESTDIR       = 

CC            = @CC@
CXX	      = @CXX@

CFLAGS        = @CFLAGS@
CXXFLAGS      = @CXXFLAGS@ 
DEFS          = -DDATADIR=\"${pkgdatadir}\" 
INCS          = -I$(builddir)
LIBS	      = @LIBS@
LIBFLAGS      = -DLIBRARY

LINKER	      = $(CXX)
LDFLAGS       = @LDFLAGS@

RM            = rm -f
INSTALL	      = @INSTALL@
MKDIRS        = $(top_srcdir)/mkinstalldirs
AR	      = @AR@
RANLIB	      = @RANLIB@

srcdir        = @srcdir@
VPATH         = @srcdir@
top_srcdir    = @top_srcdir@
top_builddir  = @top_builddir@
builddir      = @top_builddir@/src

HDRS	      = vector3.h matrix3x3.h

OBJS	      =	vector3.o matrix3x3.o

MAKEFILE      = Makefile

all:		$(OBJS)

clean:;		$(RM) $(OBJS) 
distclean:	clean

dist:		distclean

# Update the Makefile if the Makefile.in or configure script changes
Makefile:	Makefile.in
		(cd ../..; ./config.status)

.SUFFIXES:	.cpp
.cpp.o:
		$(CXX) $(CXXFLAGS) $(DEFS) $(INCS) -c $< -o $@
